{
  "manifest_version": 3,
  "name": "LeetCode Submission Fetcher",
  "version": "1.0",

  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],

  "host_permissions": [
    "https://leetcode.com/*",
    "https://api.github.com/*"
  ],

  "background": {
    "service_worker": "background.js"
  }
}
